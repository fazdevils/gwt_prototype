<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:gwt="urn:import:com.vincentfazio.ui.administration.view">
	<g:StackLayoutPanel unit='EM' ui:field="adminMenu" animationDuration="500">
		<g:stack>
			<g:customHeader size="2">
				<g:Label>Manage Suppliers</g:Label>
			</g:customHeader>
			<gwt:VendorListView ui:field="vendorList"/>
		</g:stack>
		<g:stack>
			<g:customHeader size="2">
				<g:Label>Manage Users</g:Label>
			</g:customHeader>
			<gwt:UserListView ui:field="userList"/>
		</g:stack>
	</g:StackLayoutPanel>      
</ui:UiBinder> 